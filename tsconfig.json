{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"lib": ["ESNext"],
		"types": ["bun-types"],
		"strict": true,
		"skipLibCheck": true,
		"noEmit": false,
		"declaration": true,
		"declarationMap": true,
		"outDir": "dist",
		"rootDir": "src"
	},
	"include": ["src"]
}
